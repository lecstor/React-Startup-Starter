{
  "name": "react-startup-starter",
  "version": "0.0.1",
  "description": "A good base for a startup web app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint ./src/ ./tests/",
    "build": "NODE_ENV=production webpack --config webpack/production.js",
    "build:dev": "webpack --config webpack/development.js",
    "build:stats": "webpack --config webpack/production.js --profile --json > dist/stats.json",
    "start": "NODE_PATH='src' node dev_server.js",
    "start:prod": "NODE_ENV=production npm run start",
    "test": "karma start karma.conf.js",
    "watch": "watch 'clear && npm run test -s' src",
    "cover": "babel-istanbul cover --report html node_modules/.bin/blue-tape -- src/**/*shallow.test.js",
    "test:once": "karma start karma.conf.js --single-run",
    "test:blitz": "NODE_APP_INSTANCE=osx_blitz karma start karma.conf.js --single-run",
    "test:prod": "NODE_ENV=production NODE_APP_INSTANCE=osx_blitz karma start karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lecstor/React-Startup-Starter.git"
  },
  "author": "Jason Galea <jason@lecstor.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.1.1",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "json5": "^0.4.0",
    "node-uuid": "^1.4.7",
    "react": "^0.14.5",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-router-bootstrap": "^0.19.3",
    "redux": "^3.0.5",
    "redux-form": "^4.0.5",
    "redux-simple-router": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-istanbul": "^0.5.9",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "blue-tape": "^0.1.11",
    "body-parser": "^1.14.2",
    "client-sessions": "^0.7.0",
    "config": "^1.17.1",
    "connect-history-api-fallback": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^1.2.0",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.0",
    "fetch-mock": "^3.2.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.16",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.5",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-mock-store": "0.0.4",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.0.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "tape": "^4.4.0",
    "url-loader": "^0.5.6",
    "watch": "^0.16.0",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}

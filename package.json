{
  "name": "react-startup-starter",
  "version": "0.0.1",
  "description": "A good base for a startup web app",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint ./src/ ./tests/",
    "build": "NODE_ENV=production webpack --config webpack/production.js",
    "build:dev": "webpack --config webpack/development.js",
    "build:stats": "NODE_ENV=production webpack --config webpack/production.js --profile --json > dist/stats.json",
    "start": "NODE_PATH='src' node dev_server.js",
    "start:prod": "NODE_ENV=production npm run start",
    "test": "karma start karma.conf.js",
    "watch": "watch 'clear && npm run test -s' src",
    "cover": "babel-istanbul cover --report html node_modules/.bin/blue-tape -- src/**/*shallow.test.js",
    "test:once": "karma start karma.conf.js --single-run",
    "test:blitz": "NODE_APP_INSTANCE=osx_blitz karma start karma.conf.js --single-run",
    "test:prod": "NODE_ENV=production NODE_APP_INSTANCE=osx_blitz karma start karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lecstor/React-Startup-Starter.git"
  },
  "author": "Jason Galea <jason@lecstor.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.1.1",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "json5": "^0.4.0",
    "lodash": "^4.6.1",
    "node-uuid": "^1.4.7",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-bootstrap": "^0.20.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "spin.js": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "blue-tape": "^0.2.0",
    "body-parser": "^1.14.2",
    "client-sessions": "^0.7.0",
    "config": "^1.17.1",
    "connect-history-api-fallback": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^1.3.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.0",
    "estraverse": "^4.1.1",
    "estraverse-fb": "^1.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.0",
    "fetch-mock": "^3.2.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.8.0",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-inspector": "^0.3.1",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-mock-store": "0.0.6",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.0.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "url-loader": "^0.5.6",
    "watch": "^0.17.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}
